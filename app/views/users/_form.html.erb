<%= simple_form_for @user do |f| %>


    <div>
     <%= f.input :first_name %>
    
    </div>
    <div>
     <%= f.input :last_name %>
     
    </div>
    <div>
     <%= f.input :email %>

    </div>
    <div>
     <%= f.input :password %>
     
    </div>
    <div>
     <%= f.input :password_confirmation %>
     
    </div>
    <%= f.submit class: 'btn btn-primary' %>
    <% end %>