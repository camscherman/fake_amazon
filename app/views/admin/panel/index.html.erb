<h2>Admin Panel!</h2>
    <%= @stats[:user_count] %>
    <%= @stats[:product_count] %>
    <%= @stats[:review_count] %>

    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email </th>
            <th>Admin?</th>
        </tr>
        <% @users.each do |u| %>
        <tr>
        <td><%= u.first_name %></td>
        <td><%= u.last_name %></td>
        <td><%= u.email %></td>
        <td><%= u.is_admin %></td>
        </tr>
        <% end %>
    
    </table>